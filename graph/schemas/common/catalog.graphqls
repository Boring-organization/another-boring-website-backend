type CatalogItem {
    id: String!
    code: String!
    value: String!
    text: String!
    catalog_id: String!
    isActive: Boolean!
}

type Catalog {
    id: String!
    name: String!
    isActive: Boolean!
}

enum Catalogs  {
    violationType
    kink
    sex
}

extend type Query {
    getCatalog(catalog: Catalogs): Catalog @authenticated
    getCatalogItems(catalog: Catalogs): [CatalogItem!]! @authenticated
    getCatalogItem(catalogItemCode: String!): CatalogItem! @authenticated
}
